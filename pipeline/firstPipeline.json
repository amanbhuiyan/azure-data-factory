{
	"name": "firstPipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy Characters",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureTableSink",
						"azureTableInsertType": "merge",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['count']"
								},
								"sink": {
									"name": "count"
								}
							},
							{
								"source": {
									"path": "$['next']"
								},
								"sink": {
									"name": "next"
								}
							},
							{
								"source": {
									"path": "$['previous']"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['name']"
								},
								"sink": {
									"name": "name"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['height']"
								},
								"sink": {
									"name": "height"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['vehicles'][0]"
								},
								"sink": {
									"name": "vehicle"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['created']"
								},
								"sink": {
									"name": "created"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['edited']"
								},
								"sink": {
									"name": "edited"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['url']"
								},
								"sink": {
									"name": "url"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "RestResource1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}